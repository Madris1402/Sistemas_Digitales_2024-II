module seqdiv_vlg_vec_tst();
// constants                                           
// general purpose registers
reg ck;
// wires                                               
wire [9:0] sQ;

wire sampler;                             

// assign statements (if any)                          
seqdiv i1 (
// port map - connection between master ports and signals/registers   
	.ck(ck),
	.sQ(sQ)
);

// ck
always
begin
	ck = 1'b0;
	ck = #500 1'b1;
	#500;
end 

seqdiv_vlg_sample_tst tb_sample (
	.ck(ck),
	.sampler_tx(sampler)
);

seqdiv_vlg_check_tst tb_out(
	.sQ(sQ),
	.sampler_rx(sampler)
);
endmodule

